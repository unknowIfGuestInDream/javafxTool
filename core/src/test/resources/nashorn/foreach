var list = [1, 2, 3, 4, 5];
var result = '';
for each (var i in list) {
  result+=i+'-';
}
print(result);

var ArrayList = Java.type('java.util.ArrayList');
var list = new ArrayList();
list.add('a');
list.add('b');
list.add('c');

for each (var el in list) {
  print(el);  // a, b, c
}

var map = new java.util.HashMap();
map.put('foo', 'val1');
map.put('bar', 'val2');

for each (var e in map.keySet()) {
  print(e);  // foo, bar
}

for each (var e in map.values()) {
  print(e);  // val1, val2
}
