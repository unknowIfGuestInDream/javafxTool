<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.AnchorPane?>
<ScrollPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.tlcsdm.smc.controller.SampleTreeController">
    <AnchorPane>
        <ToolBar BorderPane.alignment="CENTER">
            <Button fx:id="exportButton" mnemonicParsing="false" text="export"/>
        </ToolBar>
        <TreeView fx:id="sampleTree" BorderPane.alignment="CENTER"/>
    </AnchorPane>

</ScrollPane>
