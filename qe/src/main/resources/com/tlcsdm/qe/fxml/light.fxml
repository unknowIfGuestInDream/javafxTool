<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.shape.SVGPath?>
<?import javafx.scene.text.Font?>
<ScrollPane fitToHeight="true" fitToWidth="true" minHeight="855.0" minWidth="1155.0" stylesheets="@light.css"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.tlcsdm.qe.tools.FxmlDemo">
    <content>
        <SplitPane id="splitPane" fx:id="splitPane" dividerPositions="0.25" minHeight="850.0" minWidth="1150.0">
            <items>
                <VBox maxWidth="240.0" minWidth="140.0">
                    <children>
                        <TreeView id="treeView" fx:id="treeView" prefHeight="200.0" prefWidth="200.0"
                                  VBox.vgrow="ALWAYS"/>
                    </children>
                </VBox>
                <VBox minWidth="200.0">
                    <children>
                        <Label fx:id="lblControl" text="Title">
                            <VBox.margin>
                                <Insets left="5.0" top="3.0"/>
                            </VBox.margin>
                        </Label>
                        <Separator>
                            <VBox.margin>
                                <Insets bottom="3.0"/>
                            </VBox.margin>
                        </Separator>
                        <GridPane VBox.vgrow="ALWAYS">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="420.0" minWidth="10.0"
                                                   prefWidth="100.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" vgrow="NEVER"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
                            </rowConstraints>
                            <children>
                                <VBox minHeight="300.0" styleClass="innerVbox" GridPane.columnIndex="1"
                                      GridPane.rowIndex="3">
                                    <children>
                                        <AnchorPane VBox.vgrow="ALWAYS">
                                            <children>
                                                <Label layoutX="12.0" layoutY="10.0" text="Power Control">
                                                    <padding>
                                                        <Insets left="5.0"/>
                                                    </padding>
                                                </Label>
                                                <Button fx:id="btnPowerControl" layoutX="116.0" layoutY="6.0"
                                                        mnemonicParsing="false" onAction="#powerControlAction"
                                                        text="OFF"/>
                                                <Rectangle fx:id="rectLighting" arcHeight="5.0" arcWidth="5.0"
                                                           fill="#f0f3f5" height="236.0" layoutX="27.0" layoutY="57.0"
                                                           stroke="BLACK" width="136.0"/>
                                                <SVGPath fx:id="svgOutline" content="M40,60 C42,48 44,30 25,32"
                                                         layoutX="36.0" layoutY="109.0"/>
                                                <SVGPath fx:id="svgRay" content="M40,60 C42,48 44,30 25,32"
                                                         layoutX="36.0" layoutY="109.0"/>
                                                <SVGPath fx:id="svgLighting" content="M40,60 C42,48 44,30 25,32"
                                                         layoutX="36.0" layoutY="109.0"/>
                                                <Label layoutX="35.0" layoutY="222.0" text="Actual level:">
                                                    <font>
                                                        <Font size="10.0"/>
                                                    </font>
                                                </Label>
                                                <Label fx:id="lblActualLevel" layoutX="45.0" layoutY="245.0" text="50%">
                                                    <font>
                                                        <Font size="16.0"/>
                                                    </font>
                                                </Label>
                                                <Slider id="sliderLight" fx:id="sliderLight" blockIncrement="1.0"
                                                        layoutX="136.0" layoutY="62.0" max="254.0"
                                                        orientation="VERTICAL" prefHeight="226.0" prefWidth="14.0"
                                                        value="127.0"/>
                                                <Button fx:id="btnUp" layoutX="178.0" layoutY="70.0"
                                                        mnemonicParsing="false" onAction="#upLevelAction"
                                                        styleClass="innerButton" text="Up    " wrapText="true">
                                                    <opaqueInsets>
                                                        <Insets/>
                                                    </opaqueInsets>
                                                </Button>
                                                <Button fx:id="btnStepUp" layoutX="273.0" layoutY="70.0"
                                                        mnemonicParsing="false" onAction="#stepUpAction"
                                                        styleClass="innerButton" text="Step Up" wrapText="true"/>
                                                <Button fx:id="btnMax" layoutX="368.0" layoutY="70.0"
                                                        mnemonicParsing="false" onAction="#maxLevelAction"
                                                        styleClass="innerButton" text="Max"/>
                                                <Button fx:id="btnDown" layoutX="178.0" layoutY="159.0"
                                                        mnemonicParsing="false" onAction="#downLevelAction"
                                                        styleClass="innerButton" text="Down" wrapText="true"/>
                                                <Button fx:id="btnStepDown" layoutX="273.0" layoutY="159.0"
                                                        mnemonicParsing="false" onAction="#stepDownAction"
                                                        styleClass="innerButton" text="Step Down" wrapText="true"/>
                                                <Button fx:id="btnMin" layoutX="368.0" layoutY="159.0"
                                                        mnemonicParsing="false" onAction="#minLevelAction"
                                                        styleClass="innerButton" text="Min"/>
                                                <Button fx:id="btnDirectLevel" layoutX="273.0" layoutY="245.0"
                                                        mnemonicParsing="false" onAction="#directLevelAction"
                                                        text="Set to direct level"/>
                                                <TextField fx:id="txtDirectLevel" layoutX="178.0" layoutY="246.0"
                                                           onKeyPressed="#directLevelEnterAction" prefHeight="23.0"
                                                           prefWidth="87.0"/>
                                            </children>
                                        </AnchorPane>
                                    </children>
                                    <opaqueInsets>
                                        <Insets/>
                                    </opaqueInsets>
                                    <GridPane.margin>
                                        <Insets/>
                                    </GridPane.margin>
                                </VBox>
                                <VBox fx:id="boxChart" styleClass="innerVbox" GridPane.columnIndex="1"
                                      GridPane.rowIndex="1" GridPane.rowSpan="2" GridPane.vgrow="ALWAYS">
                                    <children>
                                        <HBox alignment="CENTER" styleClass="innerHbox">
                                            <children>
                                                <Label alignment="TOP_CENTER" text="The logarithmic dimming curve">
                                                    <padding>
                                                        <Insets left="5.0"/>
                                                    </padding>
                                                </Label>
                                            </children>
                                            <VBox.margin>
                                                <Insets bottom="10.0" left="5.0"/>
                                            </VBox.margin>
                                        </HBox>

                                    </children>
                                </VBox>
                                <VBox id="dataVbox" prefHeight="126.0" prefWidth="100.0" styleClass="innerVbox"
                                      GridPane.columnIndex="1">
                                    <children>
                                        <GridPane alignment="CENTER" VBox.vgrow="ALWAYS">
                                            <columnConstraints>
                                                <ColumnConstraints hgrow="SOMETIMES" minWidth="50.0"/>
                                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
                                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
                                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
                                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
                                            </columnConstraints>
                                            <rowConstraints>
                                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                            </rowConstraints>
                                            <children>
                                                <Label alignment="CENTER" styleClass="readOnlyDataKey" text="PHM"/>
                                                <Label styleClass="readOnlyDataKey" text="MIN LEVEL"
                                                       GridPane.columnIndex="1"/>
                                                <Label styleClass="readOnlyDataKey" text="MAX LEVEL"
                                                       GridPane.columnIndex="2"/>
                                                <Label styleClass="readOnlyDataKey" text="POWER ON LEVEL"
                                                       GridPane.columnIndex="3"/>
                                                <Label styleClass="readOnlyDataKey" text="SYSTEM FAILURE LEVEL"
                                                       GridPane.columnIndex="4"/>
                                                <Label fx:id="lblPhm" styleClass="readOnlyDataVal" text="26"
                                                       GridPane.rowIndex="1">
                                                    <font>
                                                        <Font size="20.0"/>
                                                    </font>
                                                </Label>
                                                <Label fx:id="lblMinLevel" styleClass="readOnlyDataVal" text="170"
                                                       GridPane.columnIndex="1" GridPane.rowIndex="1">
                                                    <font>
                                                        <Font size="20.0"/>
                                                    </font>
                                                </Label>
                                                <Label fx:id="lblMaxLevel" styleClass="readOnlyDataVal" text="240"
                                                       GridPane.columnIndex="2" GridPane.rowIndex="1">
                                                    <font>
                                                        <Font size="20.0"/>
                                                    </font>
                                                </Label>
                                                <Label fx:id="lblPowerOnLevel" styleClass="readOnlyDataVal" text="254"
                                                       GridPane.columnIndex="3" GridPane.rowIndex="1">
                                                    <font>
                                                        <Font size="20.0"/>
                                                    </font>
                                                </Label>
                                                <Label fx:id="lblSystemFailLevel" styleClass="readOnlyDataVal"
                                                       text="254" GridPane.columnIndex="4" GridPane.rowIndex="1">
                                                    <font>
                                                        <Font size="20.0"/>
                                                    </font>
                                                </Label>
                                            </children>
                                        </GridPane>
                                    </children>
                                </VBox>
                                <VBox id="settingVbox" prefHeight="200.0" prefWidth="100.0" styleClass="innerVbox"
                                      GridPane.hgrow="NEVER" GridPane.rowSpan="4">
                                    <children>
                                        <AnchorPane prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                                            <children>
                                                <Label layoutX="16.0" layoutY="11.0" text="Fade Setting"/>
                                                <Label layoutX="35.0" layoutY="37.0" text="Fade time:"/>
                                                <ComboBox fx:id="cmbFadeTime" layoutX="52.0" layoutY="60.0"
                                                          prefHeight="23.0" prefWidth="192.0"/>
                                                <Button fx:id="btnFadeTime" layoutX="261.0" layoutY="60.0"
                                                        mnemonicParsing="false" onAction="#fadeTimeAction"
                                                        prefWidth="46.0" styleClass="fadeButton" text="Set"/>
                                                <Label layoutX="37.0" layoutY="95.0" text="Fade rate:"/>
                                                <ComboBox fx:id="cmbFadeRate" layoutX="52.0" layoutY="119.0"
                                                          prefHeight="23.0" prefWidth="192.0"/>
                                                <Button fx:id="btnFadeRate" layoutX="261.0" layoutY="119.0"
                                                        mnemonicParsing="false" onAction="#fadeRateAction"
                                                        prefWidth="46.0" styleClass="fadeButton" text="Set"/>
                                                <Label layoutX="20.0" layoutY="176.0" text="Level Setting"/>
                                                <Label alignment="CENTER_RIGHT" layoutX="41.0" layoutY="202.0"
                                                       text="Store to:"/>
                                                <ComboBox fx:id="cmbLevelStore" layoutX="52.0" layoutY="222.0"
                                                          prefHeight="23.0" prefWidth="192.0"/>
                                                <Button fx:id="btnLevelSet" layoutX="261.0" layoutY="222.0"
                                                        mnemonicParsing="false" onAction="#levelSettingAction"
                                                        prefWidth="46.0" text="Set"/>
                                                <RadioButton fx:id="radioActual" alignment="CENTER_RIGHT" layoutX="66.0"
                                                             layoutY="268.0" mnemonicParsing="false" selected="true"
                                                             text="Actual">
                                                    <toggleGroup>
                                                        <ToggleGroup fx:id="levelGroup"/>
                                                    </toggleGroup>
                                                </RadioButton>
                                                <RadioButton fx:id="radioDirect" alignment="CENTER_RIGHT" layoutX="66.0"
                                                             layoutY="302.0" mnemonicParsing="false" text="Direct"
                                                             toggleGroup="$levelGroup"/>
                                                <TextField fx:id="txtDirect" layoutX="140.0" layoutY="299.0"
                                                           prefHeight="23.0" prefWidth="74.0"/>
                                            </children>
                                        </AnchorPane>
                                    </children>
                                </VBox>
                            </children>
                        </GridPane>
                    </children>
                </VBox>
            </items>
        </SplitPane>
    </content>
</ScrollPane>
