<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.shape.SVGPath?>
<ScrollPane fitToHeight="true" fitToWidth="true" minHeight="540.0" minWidth="640.0" stylesheets="@light.css"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.tlcsdm.qe.tools.FxmlDemo">
    <content>
        <SplitPane id="splitPane" fx:id="splitPane" dividerPositions="0.3" minWidth="-Infinity" prefHeight="400.0"
                   prefWidth="600.0">
            <items>
                <VBox minWidth="100.0" prefHeight="200.0" prefWidth="100.0">
                    <children>
                        <TreeView id="treeView" fx:id="treeView" prefHeight="200.0" prefWidth="200.0"
                                  VBox.vgrow="ALWAYS"/>
                    </children>
                </VBox>
                <VBox minWidth="200.0" prefHeight="398.0" prefWidth="406.0">
                    <children>
                        <Label fx:id="labelControl" text="Title">
                            <VBox.margin>
                                <Insets top="2.0"/>
                            </VBox.margin>
                        </Label>
                        <Separator prefWidth="200.0"/>
                        <GridPane VBox.vgrow="ALWAYS">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            </rowConstraints>
                            <children>
                                <VBox styleClass="innerVbox" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS">
                                    <children>
                                        <BorderPane VBox.vgrow="ALWAYS">
                                            <top>
                                                <HBox alignment="CENTER_LEFT" spacing="5.0"
                                                      BorderPane.alignment="CENTER">
                                                    <children>
                                                        <Label text="Power Control">
                                                            <HBox.margin>
                                                                <Insets/>
                                                            </HBox.margin>
                                                            <padding>
                                                                <Insets left="5.0"/>
                                                            </padding>
                                                        </Label>
                                                        <Button fx:id="btnOff" mnemonicParsing="false" text="OFF">
                                                            <HBox.margin>
                                                                <Insets left="5.0"/>
                                                            </HBox.margin>
                                                        </Button>
                                                    </children>
                                                    <BorderPane.margin>
                                                        <Insets bottom="10.0"/>
                                                    </BorderPane.margin>
                                                    <opaqueInsets>
                                                        <Insets/>
                                                    </opaqueInsets>
                                                    <padding>
                                                        <Insets right="5.0" top="5.0"/>
                                                    </padding>
                                                </HBox>
                                            </top>
                                            <center>
                                                <GridPane BorderPane.alignment="CENTER">
                                                    <columnConstraints>
                                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
                                                                           prefWidth="100.0"/>
                                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
                                                                           prefWidth="100.0"/>
                                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
                                                                           prefWidth="100.0"/>
                                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
                                                                           prefWidth="100.0"/>
                                                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
                                                                           prefWidth="100.0"/>
                                                    </columnConstraints>
                                                    <rowConstraints>
                                                        <RowConstraints minHeight="10.0" prefHeight="30.0"
                                                                        vgrow="SOMETIMES"/>
                                                        <RowConstraints minHeight="10.0" prefHeight="30.0"
                                                                        vgrow="SOMETIMES"/>
                                                        <RowConstraints minHeight="10.0" prefHeight="30.0"
                                                                        vgrow="SOMETIMES"/>
                                                    </rowConstraints>
                                                    <children>
                                                        <Button mnemonicParsing="false" text="Set to direct level"
                                                                GridPane.columnIndex="2" GridPane.columnSpan="2"
                                                                GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS"
                                                                GridPane.rowIndex="2" GridPane.valignment="CENTER"/>
                                                        <Slider blockIncrement="1.0" orientation="VERTICAL" value="50.0"
                                                                GridPane.columnIndex="1" GridPane.rowSpan="3">
                                                            <GridPane.margin>
                                                                <Insets left="25.0"/>
                                                            </GridPane.margin>
                                                        </Slider>
                                                        <AnchorPane GridPane.rowSpan="3">
                                                            <children>
                                                                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#f0f3f5"
                                                                           height="160.0" stroke="BLACK"
                                                                           strokeType="INSIDE" width="80.0"/>
                                                                <SVGPath fx:id="svgLighting"
                                                                         content="M40,60 C42,48 44,30 25,32"/>
                                                                <SVGPath fx:id="svgBase"
                                                                         content="M40,60 C42,48 44,30 25,32"/>
                                                            </children>
                                                        </AnchorPane>
                                                        <Button fx:id="btnUp" mnemonicParsing="false"
                                                                styleClass="innerButton" text="Up" wrapText="true"
                                                                GridPane.columnIndex="2" GridPane.halignment="CENTER"
                                                                GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER">
                                                            <GridPane.margin>
                                                                <Insets left="5.0" right="5.0"/>
                                                            </GridPane.margin>
                                                        </Button>
                                                        <Button fx:id="btnDown" mnemonicParsing="false"
                                                                styleClass="innerButton" text="Down" wrapText="true"
                                                                GridPane.columnIndex="2" GridPane.halignment="CENTER"
                                                                GridPane.hgrow="ALWAYS" GridPane.rowIndex="1"
                                                                GridPane.valignment="CENTER">
                                                            <GridPane.margin>
                                                                <Insets left="5.0" right="5.0"/>
                                                            </GridPane.margin>
                                                        </Button>
                                                        <Button fx:id="btnStepUp" mnemonicParsing="false"
                                                                styleClass="innerButton" text="Step Up" wrapText="true"
                                                                GridPane.columnIndex="3" GridPane.halignment="CENTER"
                                                                GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER">
                                                            <GridPane.margin>
                                                                <Insets left="5.0" right="5.0"/>
                                                            </GridPane.margin>
                                                        </Button>
                                                        <Button fx:id="btnStepDown" mnemonicParsing="false"
                                                                styleClass="innerButton" text="Step Down"
                                                                wrapText="true" GridPane.columnIndex="3"
                                                                GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS"
                                                                GridPane.rowIndex="1" GridPane.valignment="CENTER">
                                                            <GridPane.margin>
                                                                <Insets left="5.0" right="5.0"/>
                                                            </GridPane.margin>
                                                        </Button>
                                                        <Button fx:id="btnMax" mnemonicParsing="false"
                                                                styleClass="innerButton" text="Max"
                                                                GridPane.columnIndex="4" GridPane.halignment="CENTER"
                                                                GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER">
                                                            <GridPane.margin>
                                                                <Insets left="5.0" right="5.0"/>
                                                            </GridPane.margin>
                                                        </Button>
                                                        <Button fx:id="btnMin" mnemonicParsing="false"
                                                                styleClass="innerButton" text="Min"
                                                                GridPane.columnIndex="4" GridPane.halignment="CENTER"
                                                                GridPane.hgrow="ALWAYS" GridPane.rowIndex="1"
                                                                GridPane.valignment="CENTER">
                                                            <GridPane.margin>
                                                                <Insets left="5.0" right="5.0"/>
                                                            </GridPane.margin>
                                                        </Button>
                                                        <TextField GridPane.columnIndex="4" GridPane.hgrow="ALWAYS"
                                                                   GridPane.rowIndex="2"/>
                                                    </children>
                                                    <padding>
                                                        <Insets left="5.0" right="5.0"/>
                                                    </padding>
                                                </GridPane>
                                            </center>
                                        </BorderPane>
                                    </children>
                                    <opaqueInsets>
                                        <Insets/>
                                    </opaqueInsets>
                                    <GridPane.margin>
                                        <Insets/>
                                    </GridPane.margin>
                                </VBox>
                                <VBox prefHeight="200.0" prefWidth="100.0" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1"
                                      GridPane.rowSpan="2" GridPane.vgrow="ALWAYS">
                                    <children>
                                        <Label text="Label"/>
                                    </children>
                                </VBox>
                                <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1"
                                      GridPane.rowSpan="2"/>
                                <VBox prefHeight="200.0" prefWidth="100.0" GridPane.columnIndex="1"
                                      GridPane.rowIndex="2"/>
                            </children>
                        </GridPane>
                    </children>
                </VBox>
            </items>
        </SplitPane>
    </content>
</ScrollPane>
