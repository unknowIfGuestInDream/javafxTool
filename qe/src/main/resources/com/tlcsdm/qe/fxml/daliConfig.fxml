                                        prefWidth="210.0" styleClass="daliSubTitle_3xx"
                                        text="302: Absolute input devices"/>
                                <Button fx:id="btnOccupancySensors" layoutX="292.0" layoutY="306.0"
                                        mnemonicParsing="false" onAction="#showDetailPane" prefHeight="44.0"
                                        prefWidth="210.0" styleClass="daliSubTitle_3xx" text="303: Occupancy sensors"/>
