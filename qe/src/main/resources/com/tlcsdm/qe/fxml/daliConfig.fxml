                                        <Font size="16.0"/>
                                    </font>
                                </Button>
                                <Button fx:id="btnLedModules" layoutX="47.0" layoutY="187.0" mnemonicParsing="false"
                                        onAction="#showDetailPane" prefHeight="44.0" prefWidth="200.0"
                                        styleClass="daliSubTitle_2xx" text="207: LED modules"/>
                                <Button layoutX="138.0" layoutY="125.0" mnemonicParsing="false"
                                        onAction="#showDetailPane" text="Memory bank"/>
                                <Button fx:id="btnColourControl" layoutX="47.0" layoutY="247.0" mnemonicParsing="false"
                                        onAction="#showDetailPane" prefHeight="44.0" prefWidth="200.0"
                                        styleClass="daliSubTitle_2xx" text="209: Colour Control"/>
                                <CheckBox fx:id="enableColourColtrol" layoutX="50.0" layoutY="262.0"
                                          mnemonicParsing="false"/>
                                <CheckBox fx:id="enableLedModules" layoutX="50.0" layoutY="201.0"
                                          mnemonicParsing="false"/>
                                <CheckBox fx:id="enablePushButtons" layoutX="295.0" layoutY="201.0"
                                          mnemonicParsing="false"/>
                                <CheckBox fx:id="enableAbsoluteInputDevices" layoutX="295.0" layoutY="261.0"
                                          mnemonicParsing="false"/>
                                <CheckBox fx:id="enableOccupancySensors" layoutX="295.0" layoutY="320.0"
                                          mnemonicParsing="false"/>
                                <CheckBox fx:id="enableLightSensors" layoutX="295.0" layoutY="383.0"
                                          mnemonicParsing="false"/>
                            </children>
                        </Group>
                    </content>
                </TitledPane>
                <TitledPane collapsible="false" layoutX="28.0" layoutY="501.0" text="DALI ">
                    <content>
                        <Group layoutX="1.0" layoutY="-131.0">
                            <children>
                                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#1f93ff00" height="466.0" layoutX="28.0"
                                           layoutY="642.0" stroke="BLACK" strokeType="INSIDE" width="491.0"/>
                                <ImageView fx:id="imgBoard" fitHeight="260.0" fitWidth="456.0" layoutX="44.0"
                                           layoutY="704.0" pickOnBounds="true" preserveRatio="true"/>
                                <ComboBox fx:id="cmbBoard" layoutX="44.0" layoutY="661.0" prefWidth="150.0"/>
                                <Button layoutX="140.0" layoutY="1049.0" mnemonicParsing="false" prefHeight="41.0"
                                        prefWidth="279.0" text="Write to board"/>
                                <Label layoutX="270.0" layoutY="1004.0" text="Compiler:"/>
                                <ComboBox layoutX="158.0" layoutY="1000.0" prefHeight="25.0" prefWidth="85.0"/>
                                <ComboBox fx:id="cmbCompiler" layoutX="330.0" layoutY="1000.0" prefHeight="25.0"
                                          prefWidth="121.0"/>
                                <Label layoutX="91.0" layoutY="1004.0" text="COM Port:"/>
                            </children>
                        </Group>
                    </content>
                </TitledPane>
                <TitledPane fx:id="detailPane" collapsible="false" layoutX="585.0" layoutY="19.0"/>
            </children>
            <padding>
                <Insets bottom="5.0"/>
            </padding>
        </AnchorPane>
    </content>
</ScrollPane>
