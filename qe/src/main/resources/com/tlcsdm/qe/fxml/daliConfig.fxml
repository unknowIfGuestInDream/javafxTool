<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<ScrollPane fitToHeight="true" fitToWidth="true" minHeight="1140.0" minWidth="1500.0" pannable="true"
            stylesheets="@daliConfig.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.tlcsdm.qe.tools.DaliDemo">
    <content>
        <AnchorPane>
            <children>
                <TitledPane collapsible="false" layoutX="28.0" layoutY="19.0" text="DALI Standard Configuration">
                    <content>
                        <Group>
                            <children>
                                <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#1f96ff00" height="419.0" layoutX="28.0"
                                           layoutY="19.0" stroke="BLACK" strokeType="INSIDE" width="491.0"/>
                                <Rectangle id="daliMajorTitle_102" arcHeight="5.0" arcWidth="5.0" height="103.0"
                                           layoutX="47.0" layoutY="63.0" stroke="BLACK" strokeType="INSIDE"
                                           width="200.0">
                                </Rectangle>
                                <Label layoutX="73.0" layoutY="71.0" styleClass="daliMajorTitle"
                                       text="102: Control gear"/>
                                <Button fx:id="btnPushButtons" layoutX="292.0" layoutY="187.0" mnemonicParsing="false"
                                        onAction="#showDetailPane" prefHeight="44.0" prefWidth="210.0"
                                        styleClass="daliSubTitle_3xx" text="301: Push buttons"/>
                                <Button fx:id="btnLightSensors" layoutX="292.0" layoutY="369.0" mnemonicParsing="false"
                                        onAction="#showDetailPane" prefHeight="44.0" prefWidth="210.0"
                                        styleClass="daliSubTitle_3xx" text="304: Light sensors"/>
