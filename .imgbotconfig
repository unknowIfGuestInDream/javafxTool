{
    "schedule": "weekly", // daily|weekly|monthly
    "ignoredFiles": [
    	"*.jpg"
    ],
    "aggressiveCompression": "false", // true|false
    "compressWiki": "true", // true|false
    "minKBReduced": 200 // set reduction threshold (default to 10),
